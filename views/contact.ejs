<%- include("partials/header") -%>
<main>
    <div class="blogs-container">
        <div class="form-box">
            <h2 class="card-title">Contact</h2>
            <form id="form" action="/api/messages" method="POST">
                <input
                    type="text"
                    class="loginput"
                    name="name"
                    placeholder="name"
                    required
                /><br /><br />
                <input
                    type="email"
                    class="loginput"
                    name="email"
                    placeholder="email"
                    required
                /><br /><br />
                <input
                    type="text-area"
                    class="loginput message"
                    name="message"
                    placeholder="message"
                    required
                /><br /><br />
                <button type="submit" class="btn">Submit</button>
            </form>
        </div>
    </div>
</main>

<%- include("partials/footer") -%>
